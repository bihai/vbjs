start = expression EOF
expression = concat_expr

{{include common}}

call_expr
    = lazy_call_expr
    / braced_expression
    / value
    / plain_call_expr
lazy_call_expr
    = lazy_name LEFT_PAREN (lazy_value (comma lazy_value)* )? RIGHT_PAREN
lazy_value
    = bracketed_identifier
    / literal
lazy_name = 'Sum' _

identifier_expr = identifier_expr_itself
name = name_itself _

// optional whitespace
_ = [ \t]*
