start = expression

expression
    = binary_expression
    / value

binary_expression 
    = value concat_op expression
    / value plus_op expression

value
    = identifier
    / literal

identifier
    = identifier_name identifier_op identifier
    / identifier_name

identifier_name = '[' name ']'

name = [A-Za-z_][A-Za-z0-9_]*
literal = '"' [^"]* '"'
concat_op = '&'
plus_op = '+'
identifier_op = '!' / '.' 
