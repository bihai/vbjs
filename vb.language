start = expression !.

expression = concat_expr

concat_expr
    = add_expr (concat_op add_expr)*

add_expr
    = value (plus_op value)*

value
    = identifier_expr
    / literal

identifier_expr
    = identifier (identifier_op identifier)*

__ = [ \t]*

identifier = '[' name ']' __

name = [A-Za-z_][A-Za-z0-9_ ]*
literal = '"' literal_text '"' __
literal_text = [^"]*
concat_op = '&' __
plus_op = '+' __
identifier_op = '!' / '.'
