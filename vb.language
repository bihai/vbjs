start = expression

expression
    = add_expression
    / value

add_expression 
    = value (concat_op value)+
    / value (plus_op value)+

value
    = identifier
    / literal

identifier
    = identifier_name (identifier_op identifier_name)*

__ = [ \t]*

identifier_name = '[' name ']' __

name = [A-Za-z_][A-Za-z0-9_]*
literal = '"' literal_text '"' __
literal_text = [^"]*
concat_op = '&' __
plus_op = '+' __
identifier_op = '!' / '.'
